@favorites.map do |favorite|
  {
    id: favorite.id,
    user_id: favorite.user_id,
    game: {
    id: favorite.game.id,
    name: favorite.game.name,
    image_url: favorite.game.image_url,
    description: favorite.game.description,
    category: favorite.game.category,
    players: favorite.game.players
    }
  }
end
